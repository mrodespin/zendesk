<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">

    <route method="GET" url="/V1/customerorder/:email">
        <service class="Wagento\Zendesk\Api\CustomerOrderRepositoryInterface" method="get"/>
        <resources>
            <resource ref="Wagento_Zendesk::zendesk_api"/>
        </resources>
    </route>

</routes>